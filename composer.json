{
    "name": "wobblecode/rest-bundle",
    "type": "symfony-bundle",
    "description": "A bundle that creates REST Apis based on your current controllers actions",
    "keywords": ["symfony", "rest", "restful", "api", "content negotiation"],
    "homepage": "https://github.com/wobblecode/WobbleCodeRestBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Hdez",
            "email": "luis@wobblecode.com"
        }
    ],
    "require": {
        "php": "~7.0",
        "symfony/framework-bundle": "~3.0|~4.0",
        "symfony/dependency-injection": "~3.0|~4.0",
        "willdurand/negotiation": "~3.0",
        "doctrine/common": "~2.2",
        "jms/serializer-bundle": "dev-sf4-compat"
    },
    "require-dev": {
        "symfony/yaml": "~3.1",
        "symfony/browser-kit": "*",
        "sensio/framework-extra-bundle": "*",
        "symfony/security-csrf": "*",
        "symfony/templating": "*",
        "symfony/class-loader": "*",
        "symfony/finder": "*",
        "symfony/css-selector": "*",
        "symfony/process": "*",
        "doctrine/doctrine-bundle": "*",
        "symfony/twig-bundle": "*",
        "doctrine/orm": "*",
        "symfony/form": "*",
        "symfony/validator": "*",
        "phpunit/phpunit": "^5.3"
    },
    "suggest": {
        "jms/serializer-bundle": "~1.1.0"
    },
    "autoload": {
        "psr-4": { "WobbleCode\\RestBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\Fixtures\\": "Tests/Fixtures/" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    }
}
